#include <console.pi>
#define $fibres \
  2=>1<Int>; \
  Gend
#define $fib \
  1=>2<Int>; \
  $fibres
(nu fib: $fib)
def Fib() =
  link(2,fib,s,2);
  ( Fib()
  | s[1]>>x;
    if x <=1
    then s[1]<<1;
         end
    else link(2,fib,s1,1);
         s1[2]<<x-1;
         s1[2]>>f1;
         link(2,fib,s2,1);
         s2[2]<<x-2;
         s2[2]>>f2;
         s[1]<<f1+f2;
         end
  )
in Fib() |

link(2,console,c,2);
c[1]<<^PutString;
c[1]<<"What is n?";
c[1]<<^GetInt;
c[1]>>v;
link(2,fib,f,1);
f[2]<<v;
f[2]>>r;
c[1]<<^PutString;
c[1]<<"Fib(n) is";
c[1]<<^PutInt;
c[1]<<r;
c[1]<<^End;
end
