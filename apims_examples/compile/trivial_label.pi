#include<console.pi>
(nu a: 1=>2{^X: Gend, ^Y: Gend})
link(2,a,s,1);
link(2,console,c,1);
c[2]<<^PutString;
c[2]<<"Sending Y";
c[2]<<^End;
s[2]<<^Y;
end
|
link(2,a,s,2);
link(2,console,c,1);
c[2]<<^PutString;
s[1]>>
{ ^X: c[2]<<"Received X";
      c[2]<<^End;
      end,
  ^Y: c[2]<<"Received Y";
      c[2]<<^End;
      end
}
