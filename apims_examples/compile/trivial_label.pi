#include<console.pi>
(nu a: 1=>2{^X: Gend, ^Y: Gend})
link(2,a,s,1);
link(2,console,c,2);
c[1]<<^PutString;
c[1]<<"Sending Y";
c[1]<<^End;
s[2]<<^Y;
end
|
link(2,a,s,2);
link(2,console,c,2);
c[1]<<^PutString;
s[1]>>
{ ^X: c[1]<<"Received X";
      c[1]<<^End;
      end,
  ^Y: c[1]<<"Received Y";
      c[1]<<^End;
      end
}
