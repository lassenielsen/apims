#include<console.pi>
#include<order.pi>

c=new console(2 of 2);

iorder=new stdorder_Int(2 of 2);
sorder=new stdorder_String(2 of 2);
forder=new stdorder_Float(2 of 2);
iorder[1]<<^leq<<5<<7;
iorder[1]>>r1;
c[1]<<^str<<"5<=7? "<<^str<<if r1 then "yes" else "no" << ^nl;
sorder[1]<<^less<<"abc"<<"abba";
sorder[1]>>r2;
c[1]<<^str<<"abc<abba? "<<^str<<if r2 then "yes" else "no" << ^nl;
forder[1]<<^eq<<3.1451<<3.1415;
forder[1]>>r3;
c[1]<<^str<<"3.1451=3.1415? "<<^str<<if r3 then "yes" else "no" << ^nl;

