#define $fib \
  2->1:Int;\
  1->2:Int;\
  $end;

global $fib fib(1 pure,2);
local pure service Fib(fib (1 of 2) s)
( s[2]>>x;
  if x <=1
  then s[2]<<1;
  else s1 = new fib(2 of 2);
       s1[1]<<x-1;
       s1[1]>>f1;
       s2=new fib(2 of 2);
       s2[1]<<x-2;
       s2[1]>>f2;
       s[2]<<f1+f2;
)
|
