      local pure RotateRight($set(#t)(1 of 1 pure, 2) this, #t member, $set(#t)(2 of 1 pure, 1) left, $set(#t)(2 of 1 pure, 2) right) // {{{
      ( left[1]<<^Dissolve;
        left[1]>>lMember;
        left[1]>>lLeft;
        left[1]>>lRight;
