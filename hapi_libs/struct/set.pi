    ( local pure RotateLeft($set(#t)(1 of 1 pure, 2) this, #t member, $set(#t)(2 of 1 pure, 1) left, $set(#t)(2 of 1 pure, 2) right) // {{{
      ( right[1]<<^Dissolve;
        right[1]>>rMember;
        right[1]>>rLeft;
