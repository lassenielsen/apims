          }
         ^NonEmpty:
          left[1]>>lMember;
          left[1]>>lLeft;
          left[1]>>lRight;
          right[1]<<^Dissolve;
          right[1]>>
          {^Empty:
            righ=new set(2 of 2);
            right<<#t;
            right[1]<<chleq;
            s=new set(2 of 2);
            s<<#t;
            s[1]<<chleq;
            s[1]<<^Assign<<member<<lRight<<right;
            Set(this,lMember,lLeft,s);
           ^NonEmpty:
